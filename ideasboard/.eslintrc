{
  "root": true,
  "env": {
    "browser": true,
    "amd": true,
    "node": true
  },
  "parser": "vue-eslint-parser",
  "plugins": [
    "@typescript-eslint"
  ],
  "parserOptions": {
    "parser": "@typescript-eslint/parser",
    "sourceType": "module"
  },
  "extends": [
    "eslint:recommended",
    "plugin:@typescript-eslint/eslint-recommended",
    "plugin:@typescript-eslint/recommended"
  ],
  "rules": {
    "semi": "error",
    "quotes": [
      "error",
      "single"
    ]
  }
}
